<?xml version="1.0"?>
<layout version="0.1.0">
    <mygento_deepzoom_catalog_product_view>
    	 <reference name="head" >
    	 	<action method="addItem">
	    	 	<type>skin_js</type><name>js/mygento/seadragon-min.js</name>
    	 	</action>
    	 	<action method="addItem">
	    	 	 <type>skin_css</type><name>css/mygento/deepzoom.css</name>
    	 	</action>
        </reference>
        <!-- <remove name="product.info.media" />  -->
        <reference name="product.info">
        	<block type="catalog/product_view_media" name="product.info.media.mygento.deepzoom" as="media" template="mygento/deepzoom/media.phtml"/>
        </reference>
    </mygento_deepzoom_catalog_product_view>
    <block type="mygento_deepzoom/gallery" name="mygento.deepzoom.gallery" template="mygento/deepzoom/gallery.phtml">
	    <action method="addItem">
		 	<type>skin_js</type><name>js/mygento/seadragon-min.js</name>
	 	</action>
	 	<action method="addItem">
    	 	 <type>skin_css</type><name>css/mygento/deepzoom.css</name>
	 	</action>
    </block>
</layout>